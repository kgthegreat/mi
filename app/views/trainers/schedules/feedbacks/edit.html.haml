.page-header Edit Feedback

= form_for @feedback, :url => trainer_schedule_feedback_path(@trainer, @schedule), :html => { :class => "form-horizontal"} do |form|
  %fieldset
    = render :partial => 'form', :locals => { :form => form }
    .form-actions
      = form.submit 'Update', :class => "btn btn-primary"
      = link_to 'Cancel', trainer_schedule_feedback_path(@trainer), :class => 'btn'
